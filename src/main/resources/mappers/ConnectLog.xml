<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.albion.Mapper.ConnectLogMapper">

    <insert id="insertConnectLog" parameterType="com.web.albion.dto.ConnectLogDto">
        INSERT INTO connect_log(remote_ip, request_uri)
        VALUES (#{remote_ip}, #{request_uri})
    </insert>

</mapper>