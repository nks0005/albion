<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.albion.Mapper.UsersMapper">

    <select id="getUserByUsername" parameterType="String" resultType="com.web.albion.dto.Users">
        SELECT
            *
        FROM
            user_info
        WHERE
            username = #{username}
    </select>

</mapper>