<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.web.albion.Mapper.UserWinLosePerMatchMapper">

    <select id="getUserWinLose" parameterType="String" resultType="com.web.albion.dto.UserWinLosePerMatchDto">
        SELECT *
        FROM user_win_loss_count_per_match_type
        WHERE LOWER(user_name) = LOWER(#{user_name})
    </select>

</mapper>